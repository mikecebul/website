{
  "name": "mikecebul-website",
  "version": "0.0.1",
  "description": "Mike Cebul - Software Engineering Consultant and Full-Stack Developer website.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation payload migrate && next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "typecheck": "tsc --noEmit",
    "migrate:create": "cross-env payload migrate:create",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@payloadcms/admin-bar": "3.76.0",
    "@payloadcms/db-mongodb": "3.76.0",
    "@payloadcms/email-nodemailer": "3.76.0",
    "@payloadcms/email-resend": "3.76.0",
    "@payloadcms/live-preview-react": "3.76.0",
    "@payloadcms/next": "3.76.0",
    "@payloadcms/plugin-form-builder": "3.76.0",
    "@payloadcms/plugin-redirects": "3.76.0",
    "@payloadcms/plugin-sentry": "3.76.0",
    "@payloadcms/plugin-seo": "3.76.0",
    "@payloadcms/richtext-lexical": "3.76.0",
    "@payloadcms/storage-s3": "3.76.0",
    "@payloadcms/ui": "3.76.0",
    "@sentry/nextjs": "10.38.0",
    "@tanstack/react-form": "1.28.0",
    "@vis.gl/react-google-maps": "^1.7.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^10.1.0",
    "date-fns": "4.1.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-fade": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "geist": "^1.7.0",
    "jsonwebtoken": "9.0.3",
    "lucide-react": "^0.563.0",
    "motion": "12.34.0",
    "next": "16.1.1-canary.35",
    "next-sitemap": "4.2.3",
    "next-themes": "0.4.6",
    "payload": "3.76.0",
    "plaiceholder": "^3.0.0",
    "prism-react-renderer": "2.4.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "require-in-the-middle": "^8.0.1",
    "sharp": "0.34.5",
    "tailwind-merge": "3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "16.1.1",
    "@payloadcms/eslint-config": "3.28.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@types/escape-html": "^1.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "24.2.1",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "copyfiles": "^2.4.1",
    "eslint": "10.0.0",
    "eslint-config-next": "16.1.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
